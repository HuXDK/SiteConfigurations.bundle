<?xml version="1.0" encoding="UTF-8"?>
<site site="http://media.mtvnservices.com/"
	plugin="http://media.mtvnservices.com/.+player.+\.swf"
	initialState="playing"
	agent="Mozilla/5.0 (Macintosh; Intel Mac OS X 10_6_7) AppleWebKit/534.30 (KHTML, like Gecko) Chrome/12.0.742.100 Safari/534.30"
	windowWidth="768"
	windowHeight="432"
	version="3.0">

	<crop x="0" y="0" width="0" height="0" />

	<!-- PLAYING -->
	<state name="playing">
		<!-- Click to pause -->
		<event>
			<condition>
				<command name="pause" />
			</condition>
			<action>
				<click x="5" y="5" />
			</action>
		</event>
	</state>

	<!-- PAUSED -->
	<state name="paused">
		<event>
			<condition>
				<command name="play" />
			</condition>
			<action>
				<click x="5" y="5" />
			</action>
		</event>
	</state>

</site>