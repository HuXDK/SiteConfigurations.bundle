<?xml version="1.0" encoding="UTF-8"?>
<site site="http://media.mtvnservices.com.+#tds"
	plugin="\.swf"
	agent="Mozilla/5.0 (Macintosh; Intel Mac OS X 10_7_3) AppleWebKit/534.53.11 (KHTML, like Gecko) Version/5.1.3 Safari/534.53.10"
	initialState="playing"
	windowWidth="768"
	windowHeight="432"
	version="4.0">

	<crop x="0" y="0" width="0" height="0" />

	<!-- PLAYING -->
	<state name="playing">

		<!-- Click to pause -->
		<event>
			<condition>
				<command name="pause" />
			</condition>
			<action>
				<click x="5" y="5" />
			</action>
		</event>
	</state>

	<!-- PAUSED -->
	<state name="paused">
		<event>
			<condition>
				<command name="play" />
			</condition>
			<action>
				<click x="5" y="5" />
			</action>
		</event>
	</state>

</site>
